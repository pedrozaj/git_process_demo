<Application x:Class="A35WPFSample.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:A35WPFSample"
             xmlns:vm="clr-namespace:A35WPFSample.ViewModels"
             xmlns:vm1="clr-namespace:A35WPFSample.ViewModels"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/ToastNotifications.Messages;component/Themes/Default.xaml" />
                <ResourceDictionary Source="Resources/MyButtonStyle.xaml" />
                <ResourceDictionary Source="Resources/GlassButton.xaml" />
                <ResourceDictionary Source="Resources/MainDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
